<%= form_with model: [@test, @question], local: true do |form| %>
<%= render 'shared/errors', resourse: @question %>

  <p>
    <%= form.label :body  %> <br>
    <%= form.text_field :body %>
  </p>

  <p>
    <%= form.label :test_id  %>
    <%= form.collection_select :test_id, Test.all, :id, :title, prompt: true %>
  </p>
  <p><%= form.submit %></p>
<% end %>